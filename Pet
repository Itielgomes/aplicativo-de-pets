import java.util.Date;

public class Pet extends Doacao{
    private int idPet;
    private int idAnimal;
    private int Responsavel;
    private Date dataChegada;
    private boolean vacinado;
    private String descricao;

    public int getIdPet() {
        return idPet;
    }

    public void setIdPet(final int idPet) {
        this.idPet = idPet;
    }

    public int getIdAnimal() {
        return idAnimal;
    }

    public void setIdAnimal(final int idAnimal) {
        this.idAnimal = idAnimal;
    }

    public int getResponsavel() {
        return Responsavel;
    }

    public void setResponsavel(final int responsavel) {
        Responsavel = responsavel;
    }

    public Date getDataChegada() {
        return dataChegada;
    }

    public void setDataChegada(final Date dataChegada) {
        this.dataChegada = dataChegada;
    }

    public boolean isVacinado() {
        return vacinado;
    }

    public void setVacinado(final boolean vacinado) {
        this.vacinado = vacinado;
    }

    public String getDescricao() {
        return descricao;
    }

    public void setDescricao(final String descricao) {
        this.descricao = descricao;
    }
}
